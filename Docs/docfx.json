{
  "metadata": [
    {
      "properties": {
        "TargetFramework": "net9.0"
      },
      "src": [
        {
          "files": [
            "Singulink.IO.FileSystem/Singulink.IO.FileSystem.csproj"
          ],
          "src": "../Source"
        }
      ],
      "dest": "api",
      "memberLayout": "separatePages",
      "includeExplicitInterfaceImplementations": true,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Singulink.IO.FileSystem",
      "_appName": "File System",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.png",
      "_copyrightFooter": "Â© Singulink. All rights reserved.",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "template": [ "default", "templates/singulinkfx" ],
    "xref": [ "https://learn.microsoft.com/en-us/dotnet/.xrefmap.json" ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "disableGitFeatures": true,
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false
  }
}